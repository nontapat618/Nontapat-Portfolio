<% @messages.each do |message|%>

    <div class="row">
        <div class="col-xs-2 col-xs-offset-2">
            <%= message.name  %>
        </div>
        <div class="col-xs-2 col-xs-offset-2">
            <%= message.company %>
        </div>
        <div class="col-xs-2 col-xs-offset-2">
            <%= message.message %>
        </div>        
    </div>
<% end %>

